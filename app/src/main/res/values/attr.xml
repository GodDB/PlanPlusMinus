<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SubWatchView">
        <attr name="font" format="reference"/>
    </declare-styleable>

    <declare-styleable name="TooltipView">
        <attr name="toolTipColor" format="color"/>
        <attr name="text" format="string"/>

        <attr name="arrowDirection" format="integer">
            <enum name="top" value="2" />
            <enum name="bottom" value="3" />
            <enum name="left" value="0" />
            <enum name="right" value="1" />
        </attr>

    </declare-styleable>

</resources>